{
  "name": "llm-functions-turbo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && git clean -xdf node_modules",
    "dev": "turbo run dev",
    "lint": "turbo run lint --filter=./packages/* && manypkg check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck",
    "preinstall": "npx only-allow pnpm",
    "dev:example": "turbo run dev --filter=example --filter=llm-functions-ts "
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@llm-functions/react": "workspace:*",
    "@radix-ui/react-switch": "^1.0.3",
    "@types/node": "18.16.0",
    "dotenv": "latest",
    "prettier": "latest",
    "turbo": "^1.9.3",
    "typescript": "5.1.0-beta"
  },
  "packageManager": "pnpm@8.5.1",
  "devDependencies": {
    "@changesets/changelog-git": "^0.1.14",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1"
  }
}
